var
   in, fim, soma: Inteiro
inicio
      in <- 4
      fim <- 100
      soma <- 0
      Enquanto (in < fim) faca
               in <- (in + 2)
               soma <- (soma + in)
               Escreval (in)
               Escreval
      FimEnquanto
      Escreval ("- A soma entre esses números é igual a",soma)
fimalgoritmo
