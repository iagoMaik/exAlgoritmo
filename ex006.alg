var
   numero, antecessor, sucessor: Inteiro
inicio
      Escreval ("Digite um número: ")
      Leia (numero)
      antecessor <- numero - 1
      sucessor <- numero + 1
      Escreval
      Escreval ("Seu antecessor é o número:",antecessor)
      Escreval ("Seu sucessor é o número:",sucessor)
fimalgoritmo
