var
   Km, dias, valor: Real
inicio
      Escreva ("KM percorridos pelo cliente: ")
      Leia (Km)
      Escreval
      Escreva ("por quantos dias ? ")
      Leia (dias)
      valor <- (dias * 90) + (Km * 0.20)
      Escreval
      Escreval ("O valor total à ser pago pelo cliente é de",(dias * 90) + (Km * 0.20)," reais")
fimalgoritmo
