var
   in, fim, soma: Inteiro
inicio
      in <- 550
      fim <- 0
      Enquanto (in > fim) faca
               in <- (in - 50)
               soma <- (soma + in)
               Escreval ("-",in)
               Escreval
      FimEnquanto
      Escreval ("- A soma dos números acima é igual a",soma)
fimalgoritmo
